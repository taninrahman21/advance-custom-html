{
  "name": "custom-html",
  "version": "1.0.0",
  "description": "Write code like editor like editor.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "start": "webpack --mode=development --watch",
    "sync": "browser-sync start -p 'http://development/local/' --files '**/*.php' 'dist/*.js' 'dist/*.css'",
    "preview": "npm-run-all --parallel sync start",
    "i18n-pot": "wp i18n make-pot . languages/custom-html.pot --exclude=\"src\" && cp ./languages/custom-html.pot ./languages/custom-html-en_US.po",
    "i18n-json": "wp i18n make-json languages/ --no-purge",
    "i18n-mo": "wp i18n make-mo languages/",
    "i18n": "npm run i18n-pot && npm run i18n-json && npm run i18n-mo",
    "build": "gulp clean && npm run lint && webpack --mode=production && npm run i18n && gulp bundle"
  },
  "author": "Wasek Bellah",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@wordpress/blob": "^2.13.2",
    "@wordpress/block-editor": "^5.3.3",
    "@wordpress/blocks": "^7.0.6",
    "@wordpress/components": "^12.0.8",
    "@wordpress/compose": "^3.25.3",
    "@wordpress/data": "^4.27.3",
    "@wordpress/element": "^2.20.3",
    "@wordpress/html-entities": "^2.11.2",
    "@wordpress/i18n": "^3.20.0",
    "@wordpress/rich-text": "^3.25.3",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.27.9",
    "browserslist": "^4.16.7",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.7",
    "eslint": "^7.32.0",
    "eslint-config-node": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-webpack-plugin": "^2.5.4",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.1.0",
    "mini-css-extract-plugin": "^1.6.2",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^11.1.1",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.7.2"
  },
  "browserslist": ["> 1% or last 5 versions"]
}
